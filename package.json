{
    "name": "@ef-class/class-web",
    "version": "0.0.115",
    "scripts": {
        "ng": "ng",
        "serve": "docker-compose -f .local/docker/dev.yml up --remove-orphans",
        "ef": "ef",
        "test:cli": "export TS_NODE_PROJECT=$(pwd)/projects/command-line-tools/ef/tsconfig.json && mocha -r ts-node/register --inspect \"./projects/command-line-tools/ef/src/**/*.spec.ts\" --watch --watch-extensions ts",
        "dev:cli": "export TS_NODE_PROJECT=$(pwd)/projects/command-line-tools/ef/tsconfig.json && nodemon --watch command-line-tools/**/*.ts --exec 'node --inspect -r ts-node/register ./projects/command-line-tools/ef/src/index.ts bump'",
        "cli": "ts-node --project ./projects/command-line-tools/ef/tsconfig.json ./projects/command-line-tools/ef/src/index.ts",
        "build:docker:class-web": "docker build -f .local/docker/class-web.Dockerfile -t class-web:dev .",
        "run:docker:class-web": "docker run -v ${PWD}:/class-web -v /class-web/node_modules --name class-web -it --rm class-web:dev bash"
    },
    "private": true,
